<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>JavaWebDynamicDemo</display-name>
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>GetSystemDate</servlet-name>
		<servlet-class>GetSystemDate</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetSystemDate</servlet-name>
		<url-pattern>/GetSystemDate</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Calculation2Servlet</servlet-name>
		<servlet-class>servlet.Calculation2Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Calculation2Servlet</servlet-name>
		<url-pattern>/servlet/Calculation2Servlet</url-pattern>
	</servlet-mapping>
	<servlet>
  	<servlet-name>loginServlet</servlet-name>
  	<servlet-class>servlet.loginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>loginServlet</servlet-name>
  	<url-pattern>/servlet/loginServlet</url-pattern>
  </servlet-mapping>
</web-app>